{
  "name": "fimidx-log-files-consumer",
  "version": "0.0.1",
  "description": "Consumes log files from the file system and sends them to the Fimidx server",
  "author": {
    "email": "ywordk@gmail.com",
    "name": "softkave"
  },
  "homepage": "https://github.com/softkave/fimidx/tree/main/fimidx-log-files-consumer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softkave/fimidx.git"
  },
  "bugs": {
    "url": "https://github.com/softkave/fimidx/issues"
  },
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/{*.js,**/*.js}",
    "build/{*.js.map,**/*.js.map}",
    "build/{*.d.ts,**/*.d.ts}",
    "build/{*.d.ts.map,**/*.d.ts.map}",
    "!build/**/__tests__/**/*",
    "!build/**/testUtils/**/*"
  ],
  "license": "MIT",
  "keywords": [
    "fimidx",
    "log",
    "files",
    "consumer"
  ],
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean && rm -f tsconfig.tsbuildinfo",
    "compile": "npm run clean && tsc",
    "fix": "gts fix",
    "pretty": "prettier --write",
    "pretest": "npm run compile",
    "test": "vitest run --config=./vitest.config.ts",
    "start": "node build/index.js",
    "example": "node example.js",
    "publish:npm": "npm run clean && npm run compile && npm publish",
    "publish:local": "npm run clean && npm run compile && yalc publish",
    "show:packlist": "npm run clean && npm run compile && npm pack --dry-run"
  },
  "engines": {
    "node": ">=13"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "fimidx": "^0.3.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "@types/node": "^22.7.5",
    "gts": "^6.0.2",
    "typescript": "^5.6.3",
    "vitest": "^3.1.4"
  }
}
